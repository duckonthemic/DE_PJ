# ============================================
# Enterprise Analytics Platform - Environment Variables
# ============================================
# Copy this to .env and fill in your values

# ===================
# Source Database (E-commerce OLTP)
# ===================
SOURCE_DB_HOST=localhost
SOURCE_DB_PORT=5432
SOURCE_DB_NAME=ecommerce_source
SOURCE_DB_USER=postgres
SOURCE_DB_PASSWORD=your_source_db_password

# ===================
# Data Warehouse
# ===================
DW_DB_HOST=localhost
DW_DB_PORT=5433
DW_DB_NAME=data_warehouse
DW_DB_USER=postgres
DW_DB_PASSWORD=your_dw_db_password

# ===================
# MinIO (S3-compatible storage)
# ===================
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=your_minio_secret_key

# ===================
# Metabase
# ===================
METABASE_DB_NAME=metabase
METABASE_DB_USER=metabase
METABASE_DB_PASSWORD=your_metabase_db_password

# ===================
# Staging Path
# ===================
STAGING_PATH=./data/staging
